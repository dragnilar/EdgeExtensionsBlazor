{
  "manifest_version": 2,
  "name": "Blazor Edge New Tab",
  "description": "A new tab page for Microsoft Edge using Blazor Web Assembly",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'wasm-eval' 'sha256-v8v3RKRPmN4odZ1CWM5gw80QKPCCWMcpNeOmimNL2AA='; object-src 'self'",
  "web_accessible_resources": [
    "framework/*",
    "BrowserExtensionScripts/*",
    "content/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "search",
    "storage",
    "bookmarks",
    "chrome://favicon/*",
    "downloads"
  ],
  "icons": {
    "128": "icons/BlazorNewTab.png",
    "52": "icons/BlazorNewTab.png",
    "28": "icons/BlazorNewTab.png"
  }
}